{
  "name": "array-orm",
  "version": "0.1.0",
  "description": "Active Object Relational Mapping - A lightweight reactive data management library",
  "type": "module",
  "main": "dist/aorm.cjs",
  "module": "dist/aorm.js",
  "browser": "dist/aorm.umd.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "browser": "./dist/aorm.umd.js",
      "import": "./dist/aorm.js",
      "require": "./dist/aorm.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "rimraf dist && tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "keywords": [
    "orm",
    "data",
    "reactive",
    "state",
    "javascript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/coverage-v8": "3.1.2",
    "rimraf": "^6.0.1",
    "typescript": "~5.7.2",
    "vite": "^6.3.1",
    "vitest": "^3.1.2"
  },
  "files": [
    "dist"
  ],
  "dependencies": {
    "jsdom": "^26.1.0",
    "vite-plugin-dts": "^4.5.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/saifulriza/aorm.git"
  }
}
